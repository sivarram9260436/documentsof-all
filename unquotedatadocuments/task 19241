class Fund < ActiveRecord::Base

  has_many :lp_trades

end

class FundEvent < ActiveRecord::Base

  # relationships
  belongs_to :fund


end


class FundEventType < ActiveRecord::Base

  # relationships
  has_many :fund_events

  attr_accessible :name
  scope :by_name , lambda { |name| { :conditions => ["name = ?", name] } }
end
