	<%= content_tag(:table, {:class => "dukes", :border => "1" } ) do %>
	    <%= content_tag(:tr, { :class => "fund_events head"}) do %>
			<%= content_tag(:th, "Event") %>
			<%= content_tag(:th, "Month") %>
			<%= content_tag(:th, "Year") %>
			<%= content_tag(:th, "Amount Closed") %>
