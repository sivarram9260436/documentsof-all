<div class="section_block full_section">
	<%#debugger%>
	<h2><%= "Directoy Listing List"%></h2>
	<!--<div class="section_content">-->
	<div class="right_section_head">
		<div class="common_listing_button">
			<ul>
				<li class="button">
					<%= link_to('New Directory Listing', :controller=>"admin/directory_listings",:action=>"new") %>
				</li>
			</ul>
		</div>
	</div>
	<!--  <div class="section_content">-->
	<div class="table_content">
		<table class="list" width="100%" cellspacing="0" cellpadding="0" border="0">
			<thead>
				<tr class="titlename">
					<th class="site_sn"> S/N </th>
					<%= sort_header_tag('name',:caption=>"Company Name",:class=>"list_table") %>
					<th> Categories </th>
					<th> Premium </th>
					<th> Status </th>
					<th></th>
				</tr>
			</thead>
			<tbody id='directory_listing_list'>
				<%= render :partial => "/admin/directory_listings/search_list" %>
			</tbody>
		</table>
