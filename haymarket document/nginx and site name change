ruby@HaymarketStage:~$ cat bin/restart_all.sh 
#!/bin/sh -e
#
#Restart Script
exec 2>> ~ruby/bin/restart.log  # send stderr from restart to a log file
exec 1>&2                   # send stdout to the same log file
#set -x                      # tell sh to display commands before execution

echo "\n\n=====================================\nFresh run at: `date`"
~ruby/bin/unicorn_cms.sh start || echo "Could not start CMS Unicorns"
~ruby/bin/unicorn_wps.sh start || echo "Could not start WPS Unicorns"
sudo /home/ruby/Nginx/Nginx_Conf/nginx/sbin/nginx-all || echo "Could not start Nginx"
cd /home/ruby/Solr_Server/bin
export JAVA_HOME=/usr/lib/jvm/java-1.6.0-openjdk-amd64/jre/
./startup.sh || echo "Could not start Solr"

exit 0
